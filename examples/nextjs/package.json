    "@iconify-json/carbon": "^1.1.5",
    "@types/node": "17.0.42",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@unocss/reset": "link:../../packages/reset",
    "@unocss/webpack": "link:../../packages/webpack",
    "concurrently": "^7.2.1",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "typescript": "4.7.3",
    "unocss": "link:../../packages/unocss"
  }
